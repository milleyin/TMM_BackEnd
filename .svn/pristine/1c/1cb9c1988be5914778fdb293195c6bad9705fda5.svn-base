    <script src="__STATIC__/common/js/region.js"></script>
    <body class="mallingForm">
    <div class="headerbg"></div>
    <div class="mallingCard">
        <div class="title">{$cardInfo.name}</div>
        <div class="txt">桂味荔枝25kg+随赠5kg</div>
        <span class="ticket">卡券号：{$cardInfo.card_no}</span>
    </div>

    <div class="conForm">
        <div class="specification">选择包装规格</div>
        <div class="boxes">
            <div class="item" data-item="one">
                <div class="divLeftImg">
                <eq name="specs_id" value="1">
                    <img src="__IMG__/selboxes.png"/>
                <else />
                    <img src="__IMG__/selboxes.png" style="display: none;"/>
                </eq>
                </div>
                <div class="divLeft">6箱 x 5kg</div>
            </div>
            <div class="item" data-item="two">
                <div class="divRightImg">
                    <eq name="specs_id" value="2">
                        <img src="__IMG__/selboxes.png"/>
                    <else />
                        <img src="__IMG__/selboxes.png" style="display: none;"/>
                    </eq>
                </div>
                <div class="divRight">10箱 x 3kg</div>
            </div>
        </div>
    </div>
    <div class="triangle"></div>
    <eq name="specs_id" value="1">
        <form action="{:U('Card/editLogistics', array('fromType' => $fromType))}" method="post" id="item-one-form">
            <div id="address-one">
                <volist name="addressList" id="vo">
                <div class="addr" id="addr-one-{$vo.id}">
                    <div class="receiver">
                        <div class="receiver-name">收货人{$key + 1}</div>
                        <gt name="key" value="0">
                            <div class="del-address">删除</div>
                        </gt>
                    </div>
                    <div class="con">
                        <div class="row">
                            <div class="tit">姓名</div>
                            <input type="text" name="cnee[]" value="{$vo.cnee}" placeholder="收货人真实姓名">
                        </div>
                        <div class="row">
                            <div class="tit">手机号码 </div>
                            <input type="tel" name="phone[]" value="{$vo.phone}" placeholder="手机号或电话">
                        </div>
                        <div class="row">
                            <div class="tit">邮寄箱数</div>
                            <select class="num" name="number[]">
                                <option value="1" name="isDefault" <eq name="vo.number" value="1">selected<else /></eq>>1</option>
                                <option value="2" name="isDefault" <eq name="vo.number" value="2">selected<else /></eq>>2</option>
                                <option value="3" name="isDefault" <eq name="vo.number" value="3">selected<else /></eq>>3</option>
                                <option value="4" name="isDefault" <eq name="vo.number" value="4">selected<else /></eq>>4</option>
                                <option value="5" name="isDefault" <eq name="vo.number" value="5">selected<else /></eq>>5</option>
                                <option value="6" name="isDefault" <eq name="vo.number" value="6">selected<else /></eq>>6</option>
                            </select>
                        </div>
                        <div class="row">
                            <div class="tit">选择省份</div>
                            <select class="province" name="province[]">
                                <option value="" name="isDefault">请选择省份</option>
                            </select>
                        </div>
                        <div class="row">
                            <div class="tit">选择城市</div>
                            <select class="city" name="city[]">
                                <option value="" name="isDefault">请选择城市</option>
                            </select>
                        </div>
                        <div class="row">
                            <div class="tit">选择地区</div>
                            <select class="area" name="area[]">
                                <option value="" name="isDefault">请选择地区</option>
                            </select>
                        </div>
                        <div class="row rowarea">
                            <div class="tit">详细地址</div>
                            <textarea name="address[]" rows="2" class="detialAddr" placeholder="请详细到门牌号">{$vo.address}</textarea>
                        </div>
                    </div>
                    <script>
                        var _URL_ = '__URL__';
                        var _MODULE_ = '__MODULE__';
                        var id = "{$vo.id}";
                        var provinceId = "{$vo['province']}";
                        var cityId = "{$vo['city']}";
                        var areaId = "{$vo['area']}";

                        getRegion(provinceId, cityId, areaId, 'addr-one-' + id);
                    </script>
                </div>
                </volist>
            </div>

            <div class="btnAdd"><span class="add" id="add-one">添加收货人</span></div>
            <div class="divBtn">
                <input type="hidden" name="order_id" value="{$orderId}">
                <input type="hidden" name="card_id" value="{$cardInfo.id}">
                <input type="hidden" name="specs_id" value="1">
                <button class="submit" type="submit">确认收货地址</button>
            </div>
        </form>
    <else />
