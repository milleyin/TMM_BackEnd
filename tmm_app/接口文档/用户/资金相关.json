// 模块/方法：
// index.php?r=api/cash/index

// 传值方式：
// GET

//功能描述：
//        获取提现申请记录

// 参数

// 结果
{
    "status": 1,
    "code": 0,
    "msg": "正确",
    "data": {
        "page": { // 分页信息
            "prev": "", // 上一页的链接
            // 当前页的链接
            "selected": "http://test.365tmm.net/index.php?r=store/store_cash/index",
            // 下一页的链接
            "next": "http://test.365tmm.net/index.php?r=store/store_cash/index&page=2",
            "selectedPage": 1, // 当前第几页
            "count": 2, // 页数
            "page": "page", // 参数名
            // 分页基础链接
            "link": "http://test.365tmm.net/index.php?r=store/store_cash/index"
        },
        "list_data": [ // 提现申请记录列表
            {
                "price": "10000.00", // 提现金额
                "remark_first": "初审理由", // 备注(初)
                "remark_double": "复审理由", // 备注(复)
                "remark_submit": "最终确认的理由", // 备注(确认)
                "cash_status": { // 付款状态
                    "value": "1", //
                    "name": "成功" // 
                },
                "deposit_status": { // 审核状态
                    "value": "6", // 
                    "name": "驳回" //
                },
                "add_time": "2015/09/06 12:54:35", // 添加时间
                "up_time": "2015/09/06 12:54:35" // 更新时间
            }
        ]
    }
}



